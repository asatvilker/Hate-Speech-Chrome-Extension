{
    "name": "Omnibox - New Tab Search",
    "description": "Type 'nt' plus a search term into the Omnibox to open search in new tab.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "webNavigation", 
        "tabs", 
        "<all_urls>"
    ],
    "omnibox": { "keyword" : "nt" },
    "action": {
      "default_icon": {
        "16": "whiteyeexample.png",
        "32": "whiteyeexample.png"
        }
    },
    "icons": {
      "16": "whiteyeexample.png",
      "32": "whiteyeexample.png",
      "48": "whiteyeexample.png",
      "128": "whiteyeexample.png"
    }
  }